<div class="row">
	<h1>mutualmap</h1>
	<h2>for the MakerSquare Holiday Party</h2>
	<h4>Find mutual friendships at this party!</h4>

	<%= form_for [@graph, @entrance], remote: true do |f| %>
	  <%= f.telephone_field :phone, id: 'phone-input', class: "large-12 columns" %>
	  <%= f.hidden_field :facebook_token %>
	  <%= f.submit "Send to server", :style => "display:none", id:'hidden_submit'   %>
	<% end %>
	<button id="loginbutton">Sign-in with Facebook</button>
</div>